<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/main_navigation"
  app:startDestination="@id/petsFragment">

  <fragment
    android:id="@+id/petsFragment"
    android:name="com.soyaaroncervantes.calmvet.view.fragments.PetsFragment"
    android:label="PetsFragment" >
    <action
      android:id="@+id/action_petsFragment_to_addPetFragment"
      app:destination="@id/addPetFragment" />
  </fragment>
  <fragment
    android:id="@+id/addPetFragment"
    android:name="com.soyaaroncervantes.calmvet.view.fragments.AddPetFragment"
    android:label="AddPetFragment" >
    <action
      android:id="@+id/action_addPetFragment_to_petPhotosFragment"
      app:destination="@id/petPhotosFragment" />
  </fragment>
  <fragment
    android:id="@+id/petPhotosFragment"
    android:name="com.soyaaroncervantes.calmvet.view.fragments.PetPhotosFragment"
    android:label="fragment_pet_photos"
    tools:layout="@layout/fragment_pet_photos" >
    <action
      android:id="@+id/action_petPhotosFragment_to_petsFragment"
      app:destination="@id/petsFragment" />
  </fragment>
</navigation>