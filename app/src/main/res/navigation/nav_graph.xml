<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
  app:startDestination="@id/loginFragment">
    <fragment
      android:id="@+id/loginFragment"
      android:name="com.soyaaroncervantes.calmvet.fragments.access.LoginFragment"
      android:label="LoginFragment" >
        <action
          android:id="@+id/action_loginFragment_to_registerFragment"
          app:destination="@id/registerFragment" />
        <action
          android:id="@+id/action_loginFragment_to_petFragment"
          app:destination="@id/petFragment" />
    </fragment>
    <fragment
      android:id="@+id/personalDataFragment"
      android:name="com.soyaaroncervantes.calmvet.fragments.access.PersonalDataFragment"
      android:label="PersonalDataFragment" >
        <action
          android:id="@+id/action_personalDataFragment_to_petFragment"
          app:destination="@id/petFragment" />
    </fragment>
    <fragment
      android:id="@+id/registerFragment"
      android:name="com.soyaaroncervantes.calmvet.fragments.access.RegisterFragment"
      android:label="RegisterFragment" >
        <action
          android:id="@+id/action_registerFragment_to_loginFragment"
          app:destination="@id/loginFragment" />
        <action
          android:id="@+id/action_registerFragment_to_personalDataFragment"
          app:destination="@id/personalDataFragment" />
    </fragment>
    <fragment
      android:id="@+id/petFragment"
      android:name="com.soyaaroncervantes.calmvet.fragments.app.PetFragment"
      android:label="fragment_pet_list"
      tools:layout="@layout/fragment_pet_list" >
        <action
          android:id="@+id/action_petFragment_to_detailPetFragment"
          app:destination="@id/detailPetFragment" />
    </fragment>
    <fragment
      android:id="@+id/detailPetFragment"
      android:name="com.soyaaroncervantes.calmvet.DetailPetFragment"
      android:label="fragment_detail_pet"
      tools:layout="@layout/fragment_detail_pet" />
</navigation>